{
    "name": "boshurik/mapper-bundle",
    "description": "Mapper bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "BoShurik",
            "email": "boshurik@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2 | ^8.0",
        "ext-json": "*",
        "boshurik/mapper": "^0.2",
        "symfony/framework-bundle": "^4.0|^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "nikic/php-parser": "^4.2",
        "symfony/console": "^4.0|^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^8.0"
    },
    "suggest": {
        "ext-json": "For code (models and mappings) generation",
        "symfony/console": "For code (models and mappings) generation",
        "nikic/php-parser": "For code (models and mappings) generation"
    },
    "autoload": {
        "psr-4": {
            "BoShurik\\MapperBundle\\": "src/"
        }
    },
    "autoload-dev":  {
        "psr-4": {
            "BoShurik\\MapperBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
